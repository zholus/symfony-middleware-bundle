  "require": {
    "php": ">=8.0.2",
    "symfony/framework-bundle": "^6.0",
    "symfony/event-dispatcher": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.0",
    "symfony/var-dumper": "^6.0",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
